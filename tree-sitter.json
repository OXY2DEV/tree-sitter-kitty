{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "kitty",
      "camelcase": "Kitty",
      "title": "Kitty",
      "scope": "source.kitty",
      "file-types": [
        "kitty.conf"
      ],
      "injection-regex": "^kitty$",
      "class-name": "TreeSitterKitty"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for kitty.conf files.",
    "authors": [
      {
        "name": "MD. Mouinul Hossain",
        "email": "mdmouinulhossainshawon@gmail.com",
        "url": "https://github.com/OXY2DEV/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-kitty"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
