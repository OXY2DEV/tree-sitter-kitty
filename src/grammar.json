{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "kitty",
  "rules": {
    "configuration_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_common_node"
      }
    },
    "_common_node": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment"
            },
            {
              "type": "SYMBOL",
              "name": "include"
            },
            {
              "type": "SYMBOL",
              "name": "option"
            },
            {
              "type": "SYMBOL",
              "name": "keyboard_shortcut"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n+"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[^\\n\\r]*"
          },
          "named": true,
          "value": "comment_content"
        }
      ]
    },
    "include": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_path_include"
        },
        {
          "type": "SYMBOL",
          "name": "_glob_include"
        },
        {
          "type": "SYMBOL",
          "name": "_gen_include"
        },
        {
          "type": "SYMBOL",
          "name": "_env_include"
        }
      ]
    },
    "_path_include": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "include"
        },
        {
          "type": "FIELD",
          "name": "path",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^\\s]+"
            },
            "named": true,
            "value": "path"
          }
        }
      ]
    },
    "_glob_include": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "globinclude"
        },
        {
          "type": "FIELD",
          "name": "glob",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^\\s]+"
            },
            "named": true,
            "value": "pattern"
          }
        }
      ]
    },
    "_gen_include": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "geninclude"
        },
        {
          "type": "FIELD",
          "name": "file",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^\\s]+"
            },
            "named": true,
            "value": "path"
          }
        }
      ]
    },
    "_env_include": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "envinclude"
        },
        {
          "type": "FIELD",
          "name": "environment_variable",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^\\s]+"
            },
            "named": true,
            "value": "name"
          }
        }
      ]
    },
    "option": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[\\w_]+"
            },
            "named": true,
            "value": "string"
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT1",
              "content": {
                "type": "SYMBOL",
                "name": "_primitive"
              }
            },
            "named": true,
            "value": "value"
          }
        }
      ]
    },
    "_primitive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pixel"
        },
        {
          "type": "SYMBOL",
          "name": "percentage"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "color"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "PREC",
          "value": -100,
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "."
            },
            "named": true,
            "value": "inherit"
          }
        }
      ]
    },
    "pixel": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\d+px"
      }
    },
    "percentage": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[\\d\\.]+%"
      }
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[\\d\\.\\-]+"
      }
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "yes"
        },
        {
          "type": "STRING",
          "value": "no"
        },
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "color": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "#[0-9A-Fa-f]{3,6}"
      }
    },
    "string": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\S+"
      }
    },
    "keyboard_shortcut": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "map"
        },
        {
          "type": "FIELD",
          "name": "sequence",
          "content": {
            "type": "SYMBOL",
            "name": "key_sequence"
          }
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]+"
        },
        {
          "type": "FIELD",
          "name": "action",
          "content": {
            "type": "SYMBOL",
            "name": "key_action"
          }
        }
      ]
    },
    "key_action": {
      "type": "PATTERN",
      "value": "[^\\r\\n]+"
    },
    "key_sequence": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_key_element"
      }
    },
    "_key_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "ctrl"
        },
        {
          "type": "SYMBOL",
          "name": "alt"
        },
        {
          "type": "SYMBOL",
          "name": "shift"
        },
        {
          "type": "SYMBOL",
          "name": "function"
        },
        {
          "type": "SYMBOL",
          "name": "super"
        },
        {
          "type": "SYMBOL",
          "name": "special"
        },
        {
          "type": "SYMBOL",
          "name": "key"
        },
        {
          "type": "SYMBOL",
          "name": "add"
        }
      ]
    },
    "ctrl": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "ctrl"
        },
        {
          "type": "STRING",
          "value": "control"
        },
        {
          "type": "STRING",
          "value": "^"
        }
      ]
    },
    "alt": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "alt"
        },
        {
          "type": "STRING",
          "value": "opt"
        },
        {
          "type": "STRING",
          "value": "option"
        }
      ]
    },
    "shift": {
      "type": "STRING",
      "value": "shift"
    },
    "function": {
      "type": "PATTERN",
      "value": "f[1-9]"
    },
    "super": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "super"
        },
        {
          "type": "STRING",
          "value": "cmd"
        },
        {
          "type": "STRING",
          "value": "command"
        }
      ]
    },
    "special": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "escape"
        },
        {
          "type": "STRING",
          "value": "tab"
        },
        {
          "type": "STRING",
          "value": "insert"
        },
        {
          "type": "STRING",
          "value": "left"
        },
        {
          "type": "STRING",
          "value": "up"
        },
        {
          "type": "STRING",
          "value": "page_up"
        },
        {
          "type": "STRING",
