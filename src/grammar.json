{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "kitty",
  "rules": {
    "configuration_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_common_node"
      }
    },
    "_common_node": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment"
            },
            {
              "type": "SYMBOL",
              "name": "include"
            },
            {
              "type": "SYMBOL",
              "name": "option"
            },
            {
              "type": "SYMBOL",
              "name": "keyboard_shortcut"
            },
            {
              "type": "SYMBOL",
              "name": "mouse_action"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n+"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[^\\n\\r]*"
          },
          "named": true,
          "value": "comment_content"
        }
      ]
    },
    "_primitive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pixel"
        },
        {
          "type": "SYMBOL",
          "name": "percentage"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "color"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "PREC",
          "value": -100,
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "."
            },
            "named": true,
            "value": "inherit"
          }
        }
      ]
    },
    "pixel": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\d+px"
      }
    },
    "percentage": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[\\d\\.]+%"
      }
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[\\d\\.\\-]+"
      }
    },
    "boolean": {
