      "value": "[a-z]"
    },
    "add": {
      "type": "STRING",
      "value": "+"
    }
  },
  "extras": [
    {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          },
          {
            "type": "STRING",
            "value": "\\"
          }
        ]
      }
    },
